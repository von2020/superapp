<header class="header">
  <div class="logo">
    <h1>TELNET</h1>
  </div>
  <div class="info">
    <p>News user? <span class="blue"><a href="/login">Login</a></span></p>
  </div>
</header>
<!-- front-page-container -->

<body class="">
  <div class="row">
  <div class="col-sm-5 side-img">
    <img src="images/bg.jpg" alt="">
  </div>
  <div class="col-sm-7">
    <div class="form-container">
      <form class="" action="/register" method="post">
        <h1>Authorize Account</h1>
        <div class="form-line"></div>
        <p>Or sign in using your email address</p>
        <%- include('./partials/messages')%>

        <div class="form-group form-top">
          <div class="form-item">
            <label for="login-email">Your Email</label>
            <input type="email" name="email" id="email" value='<%=id%>'  readonly>
          </div>
          <div class="form-item">
            <label for="login-email">Enter Assigned Password</label>
            <input type="password" name="old_password" id="old_password" placeholder="enter password from email">
          </div>
        </div>

        <div class="form-group form-top">
          <div class="form-item">
            <label for="password">Password</label>
            <input type="password" name="password" id="password" value="" placeholder="enter password" required>
          </div>
          <div class="form-item">
            <label for="password-confirm">Confirm Password</label>
            <input type="password" name="confirm_password" id="confirmpassword" value="" placeholder="confirm password" required>
          </div>
        </div>







        <button type="submit" class="form-button">Sign up</button>
      </form>
    </div>
  </div>
  </div>


