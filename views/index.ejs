<header class="header">
  <div class="logo">
    <h1>Telnet SuperApp</h1>
  </div>
  <div class="info">
    <p>News user? <span class="blue"><a href="/register">Create an account</a></span></p>
  </div>
</header>

<body class="front-page-container">
  <div class="row">
  <div class="col-sm-5 side-img">
    <img src="images/bg.jpg" alt="">
  </div>
  <div class="col-sm-7">
    <div class="form-container">
      <form class="" action="/login" method="post">
        <h1>Sign in to Telnet Super App</h1>
        <div class="form-line"></div>
        <p>Or sign in using your email address</p>
        <%- include('./partials/messages')%>
        <div class="form-group form-top">
          <div class="form-item">
            <label for="login-email">Your Email</label>
            <input type="email" name="username" id="login-email" value="" placeholder="example@email.com">
          </div>
          <div class="form-item">
            <label for="login-password">Password</label>
            <input type="password" name="password" id="login-password" value="" placeholder="">
          </div>
        </div>
        <div class="form-group form-bottom">
          <div class="form-item checkbox">
            <input type="checkbox" id="remember-me" name="" value="">
            <label for="remember-me">Remember Me</label>
          </div>
          <div class="form-item">
            <a><p class="link">Forgot Password?</p></a>
          </div>
        </div>
        <button type="submit" class="form-button">Log in</button>
      </form>
    </div>
  </div>
  </div>

