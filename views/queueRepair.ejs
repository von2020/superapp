<body>
	<div class="main-wrapper">

        <!-- Insert the sidebar partial here-->
        <% if (userDetails.role == 'Staff') { %>
			<%- include('./bars/sidebar_staff') %>
			<% } else if (userDetails.role == 'Supervisor') { %>
			<%- include('./bars/sidebar_sup') %>  
            <% } else if (userDetails.role == 'Auditor') { %>
            <%- include('./bars/sidebar_auditor') %> 
            <% } else if (userDetails.role == 'Finance') { %>
            <%- include('./bars/sidebar_finance') %>
			<%  } else if (userDetails.role == 'Driver Admin') { %>
			<%- include('./bars/sidebar_driver') %>  
			<%  } else if (userDetails.role == 'Driver') { %>
			<%- include('./bars/sidebar_carDriver') %>
			<%  } else if (userDetails.role == 'Director') { %>
			<%- include('./bars/sidebar_director') %>
			<%  } else if (userDetails.role == 'Group Managing Direc') { %>
			<%- include('./bars/sidebar_gmd') %>  
			<%  } else { %>
			<%- include('./bars/sidebar') %>    
			<%  } %>
	
		<div class="page-wrapper">
				
        <!-- partial:partials/_navbar.html, this represents the topbar bar -->
        <%- include('./bars/topbar') %>
        <!-- partial -->

			<div class="page-content">
                <%- include('./partials/messages')%>
								
				<div class="row">
					<div class="col-md-12 grid-margin stretch-card">
						<div class="card">
							<div class="card-body">
								<h6 class="card-title">Repair Queue</h6>

								
                                <form class="" action="/admin/manage/add-department" method="post">
                                    
                                    <div class="row">
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Recommended</label>
                                                <input name= "company_name" class="form-control" id="company_name"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Repair Date</label>
                                                <input name="fault_date" type="date" value="2020-01-01" class="form-control" id="purpose" required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Approval</label>
                                                <select class="form-control-lg" id="duration" name="speciality" required>
                                                    <option value="mechanic" selected>Mechanic</option>
                                                    <option value="rewire">Rewire</option>
                                                    <option value="panel_beater">Panel Beater</option>
                                                    <option value="welder">Welder</option>
                                                    <option value="painter">Painter</option>
                                                    <option value="air_con">Air Conditioning</option>
                                                </select>
                                            </div>
                                        </div>
                                        
									  </div>
									  
									  <div class="row">
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Comment</label>
                                                <textarea name= "address" class="form-control" id="address"  required></textarea>
                                            </div>
                                        </div>
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Auditor Name</label>
                                                <input name= "name" class="form-control" id="name"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Finance Approval </label>
                                                <select class="form-control-lg" id="duration" name="speciality" required>
                                                    <option value="mechanic" selected>Mechanic</option>
                                                    <option value="rewire">Rewire</option>
                                                    <option value="panel_beater">Panel Beater</option>
                                                    <option value="welder">Welder</option>
                                                    <option value="painter">Painter</option>
                                                    <option value="air_con">Air Conditioning</option>
                                                </select> 
                                            </div>
                                        </div>
                                        
                                    </div>
                                    <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Finance Comment</label>
                                                <textarea name= "address" class="form-control" id="address"  required></textarea>
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Finance Personnel</label>
                                                <input name= "contact_person" class="form-control" id="contact_person"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Repair Date</label>
                                                <input name="fault_date" type="date" value="2020-01-01" class="form-control" id="purpose" required> 
                                            </div>
                                        </div>
                                    </div>
                                        
									  

									  

									 
									  
									  
									  
				
									  
								<!-- need to come back here to add the list of all the subsidiaries and uplines in case it needs to be changed, just copy from the old registration page-->
									<button class="btn-lg btn-primary" type="submit">Save</button>
									
								</form>
								
							</div>
						</div>
					</div>
				</div>

			</div>
			
			<!-- partial:partials/_footer.html -->
            <%- include('./bars/footer') %>
			<!-- partial -->
	
		</div>
