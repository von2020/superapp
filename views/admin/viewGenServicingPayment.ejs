<body onload="changeFun()">
    <div class="main-wrapper">

        <!-- Insert the sidebar partial here-->
        <%- include('./bars/admin_sidebar') %>
	
		<div class="page-wrapper">
				
        <!-- partial:partials/_navbar.html, this represents the topbar bar -->
        <%- include('./bars/admin_topbar') %>
        <!-- partial -->

			<div class="page-content">
                <%- include('.././partials/messages')%>

                <nav class="page-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Generator</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Servicing Payment</li>
                    </ol>
                </nav>


                <div class="row">
                    <div class="col-md-12 grid-margin stretch-card">
                        <div class="card card-special">
                            <div class="card-body">
                                <div class="row justify-content-between">
                                    <div>
                                        <h6 class="card-title d-block">Generator Servicing Payment</h6>
                                    </div>

                                    <div class="d-flex align-items-center flex-wrap text-nowrap ">
                                        <div class="input-group date datepicker dashboard-date mr-2 mb-2 mb-md-0 d-md-none d-xl-flex"
                                            id="dashboardDate">
                                            <span class="input-group-addon bg-transparent"><i data-feather="calendar"
                                                    class=" text-primary"></i></span>
                                            <input type="text" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <form class="" action="/admin/manage/viewGenServicePayment" method="post">
                                <div class="table-responsive">
                                    <table  class="table">

                                        <thead>
                                            <tr>
                                                <th> Value </th>
                                                <th> Description </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <th> Servicing Details </th>
                                                <td>
                                            <% gens.servicing_details.forEach((item) => { %>
                                                
                                                    <p> <%= item.amount_charged%> </p>
                                                                                             
                                                     
                                                <p><a class="text-primary" href="<%= item.invoice%>">view invoice</a> </p><hr>
                                                
                                            
                                            <% }) %>
                                        </td>
                                                
                                    </tr>
                                            <hr>
                                            <tr>
                                                <th> Created By </th>
                                                <td> <%= gens.created_by%> </td>
                                            </tr>
                                            <tr>
                                                <th> Within SLA? </th>
                                                <td> <%= gens.within_sla%> </td>
                                            </tr>

                                            <tr>
                                                <th> Auditor Approval </th>
                                                <td> <%= gens.auditor_approval%> </td>
                                            </tr>
                                            <tr>
                                                <th> Auditor Name </th>
                                                <td> <%= gens.auditor_name%> </td>
                                            </tr>
                                            <tr>
                                                <th> Finance Approval </th>
                                                <td> <%= gens.finance_approval%> </td>
                                            </tr>
                                            <tr>
                                                <th> Finance Name </th>
                                                <td> <%= gens.finance_name%> </td>
                                            </tr>
                                            <tr>
                                                <th> Servicing Date </th>
                                                <td> <%= gens.servicing_date.slice(0,10)%> </td>
                                            </tr>
                                            <tr>
                                                <th> Created On </th>
                                                <td> <%= gens.created_on.slice(0,10)%> </td>
                                            </tr>

                                            <tr>
                                                <th> Updated On </th>
                                                <td> <%= gens.updated_on.slice(0,10)%> </td>
                                            </tr>
                                            

                                            

                                            
                                            
                                            

                                            

                                            
                                            
                                            

                                            

                                            

                                            
                                        </tbody>
                                        

                                        <!-- you may need to come back to change the outlook of the table to make it a two column table-->
                                    </table>
                                    <br>
                                    <br>
                                    <input type="hidden" name="serviced" class="form-control" id="serviced" value="<%=gens.id%>" required>
                                    
                                    <input type="hidden" name="created_by" class="form-control" id="created_by" value="<%=userDetails.first_name + ' ' + userDetails.last_name%>" required>

                                    

                                    
                                    
                                </div>
                                <!-- <button class="btn-lg btn-primary" type="submit">Create Status</button> -->
                                <%if (gens.servicing_status == 'Servicing status has not been created yet!') { %>
                                    <%if ((gens.auditor_approval == 'APPROVED' && gens.finance_approval == 'APPROVED')) { %>
                                    <button class="btn-lg btn-danger" type="submit"> CLICK to Create Servicing Status</button>
                                    <% } else{ %> 
    
                                        <% } %>
                                <% } else{ %> 
                                    <a class="badge badge-primary">Servicing Status Created</a>
                                    <% } %>
                            </form>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <!-- partial:partials/_footer.html -->
            <%- include('./bars/admin_footer') %>
			<!-- partial -->

        </div>
    </div>

    <!-- core:js -->
    <script src="../../../assets/vendors/core/core.js"></script>
    <!-- endinject -->
