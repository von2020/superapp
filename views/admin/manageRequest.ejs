				<div class="row">
					<div class="col-md-12 grid-margin stretch-card">
            <div class="card card-special">
              <div class="card-body">
                <div class="row justify-content-between">
                  <div>
                  <h6 class="card-title d-block">All Requests</h6>
                  <p class="card-description d-block">Latest Request (Showing 01 to 08 of 48 Tickets)</p>
                  </div>

                  <div class="d-flex align-items-center flex-wrap text-nowrap ">
                    <div class="input-group date datepicker dashboard-date mr-2 mb-2 mb-md-0 d-md-none d-xl-flex" id="dashboardDate">
                      <span class="input-group-addon bg-transparent"><i data-feather="calendar" class=" text-primary"></i></span>
                      <input type="text" class="form-control">
                    </div>
                    <!-- <button type="button" class="btn btn-primary btn-icon-text mr-2 d-none d-md-block">
                      Car Request
                    </button>
                    <button type="button" class="btn btn-outline-primary btn-icon-text mr-2 mb-2 mb-md-0">
                      Gen Request
                    </button>
                    <button type="button" class="btn btn-outline-primary btn-icon-text mb-2 mb-md-0">
                      Facilities Request
                    </button> -->
                  </div>
                </div>
                <div class="table-responsive">
                  <table id="dataTableExample" class="table" data-order=''>
                    <thead>

                      <tr>
                        <th>Staff Name</th>
                        <th>Role</th>
                        <th>Request Type</th>
                        <th>Priority</th>
                        <th>Status</th>
                        <th>View Details</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% data.forEach((item) => {%>
                    <!-- here you have to come back to add the pill thing to this page if it is accepted pending or rejected-->
                        <tr>
                          <td><%= item.requester_name%></td>
                          <td><%= item.requester_role %></td>
                          <td><%= item.request_type%></td>
                          <%if (item.priority == 1) { %>
                            <td ><span class="badge badge-info">LOW</span></td>
                          <% } else if(item.priority == 2) { %>
                            <td ><span class="badge badge-primary">MEDIUM</span></td>
                          <% } else{ %>
                            <td ><span class="badge badge-danger">HIGH</span></td>
                          <% } %>
                          <td><button class="btn btn-danger rounded-pill">Pending</button></td>
                          <td> <a href="/admin/manage/viewindividual_request?id=<%=item.id%>" id='<%=item.id%>' class="btn btn-sm btn-success rounded-pill" > View request</button></td>
                            <% })%>
                          </tr>
                    </tbody>
                  </table>                    
                </div>
              </div>
            </div>
