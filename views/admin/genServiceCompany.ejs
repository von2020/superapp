<body>
	<div class="main-wrapper">

    <%- include('./bars/admin_sidebar') %>
		<!-- partial -->
	
		<div class="page-wrapper">
				
			<!-- partial:partials/_navbar.html, this represents the topbar bar -->
        <%- include('./bars/admin_topbar') %>
			<!-- partial -->

			<div class="page-content">
        <%- include('.././partials/messages')%>

				<nav class="page-breadcrumb">
					<ol class="breadcrumb">
						<li class="breadcrumb-item"><a href="#">Generator</a></li>
						<li class="breadcrumb-item active" aria-current="page">Generator</li>
					</ol>
        </nav>


				<div class="row">
					<div class="col-md-12 grid-margin stretch-card">
            <div class="card card-special">
              <div class="card-body">
                <div class="row justify-content-between">
                  <div>
                  <h6 class="card-title d-block">Generator Service Company List</h6>
                  <!-- <p class="card-description d-block">Latest Request (Showing 01 to 08 of 48 Tickets)</p> -->
                  </div>

                </div>
                <div class="table-responsive">
                  <table id="dataTableExample" class="table">
                    <thead>

                      <tr>
                        
                        <th>Company Name</th>
                        <th>Specialty</th>
                        <!-- <th>Address</th> -->
                        <th> Email </th>
                        <!-- <th> Registration Number </th> -->
                        <!-- <th> Official Phone Number </th> -->
                        <!-- <th> Years Of Experience </th> -->
                        <th> Contact Person </th>
                        <th> Contact Person Phone </th>
                      </tr>
                    </thead>
                    <tbody>
                      
                    <!-- here you have to come back to add the pill thing to this page if it is accepted pending or rejected-->

                    <% gen_tech.forEach((item) => {%>
                        <tr>
                          <td><%= item.company_name%></td>
                          <td><%= item.specialty%></td>
                          <!-- <td><%= item.address%></td> -->
                          <td><%= item.email%></td>
                          <!-- <td><%= item.registration_number%></td> -->
                          <!-- <td><%= item.official_phone%></td> -->
                          <!-- <td><%= item.years_of_experience%></td> -->
                          <td><%= item.contact_person%></td>
                          <td><%= item.contact_person_phone%></td>
                          
                        </tr>
                        <% })%>
                    </tbody>
                  </table>
                </div>
                <div style="float:right; font-size: 2rem; color:grey;"><a href="/admin/manage/addGenTechnician"><i class="fa fa-plus fa-10x" aria-hidden="true"></a></i></div>
              </div>
            </div>
					</div>
				</div>

			</div>

			<!-- partial:partials/_footer.html -->
      <%- include('./bars/admin_footer') %>
			<!-- partial -->
	
		</div>
	</div>

	<!-- core:js -->
	<script src="../../../assets/vendors/core/core.js"></script>
	<!-- endinject -->
