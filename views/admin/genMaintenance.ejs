<body>
	<div class="main-wrapper">

        <%- include('./bars/admin_sidebar') %>
		<!-- partial -->
	
		<div class="page-wrapper">
				
			<!-- partial:partials/_navbar.html, this represents the topbar bar -->
        <%- include('./bars/admin_topbar') %>
			<!-- partial -->

			<div class="page-content">
        <%- include('.././partials/messages')%>
								
				<div class="row">
					<div class="col-md-12 grid-margin stretch-card">
						<div class="card">
							<div class="card-body">
								<h6 class="card-title">Generator Maintenance</h6>

								
                                <form class="" action="/admin/manage/genMaintenance" method="post">
                                    
                                    <div class="row">
                                        <!-- <div class="col">
                                            <div class="form-group"> -->
                                                
                                                <input name= "generator" type="hidden" class="form-control" id="generator"  required> 
                                            <!-- </div>
                                        </div> -->
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"> Check Switch Breaker</label>
                                                <select class="form-control-lg" id="check_switch_breaker_position" name="check_switch_breaker_position" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="on">on</option>
                                                    <option value="off">off</option>
                                                    
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col">
                                            <label for="exampleFormControlSelect1"> Fuel Level </label>
                                            <select class="form-control-lg" id="fuel_level" name="fuel_level" required>
                                                <option selected disabled>-----</option>
                                                <option value="full">full</option>
                                                <option value="half_full">half full</option>
                                                <option value="half_empty">half empty</option>
                                                <option value="empty">empty</option>
                                            </select>
                                        </div>
									  </div>
									  
									  <div class="row">
										<div class="col">
                                            <label for="exampleFormControlSelect1"> Oil Level</label>
                                            <select class="form-control-lg" id="oil_level" name="oil_level" required>
                                                <option selected disabled>-----</option>
                                                <option value="full">full</option>
                                                <option value="half_full">half full</option>
                                                <option value="half_empty">half empty</option>
                                                <option value="empty">empty</option>
                                            </select>
                                        </div>
                                        <div class="col">
                                            <label for="exampleFormControlSelect1"> Coolant Level</label>
                                            <select class="form-control-lg" id="coolant_level" name="coolant_level" required>
                                                <option selected disabled>-----</option>
                                                <option value="acceptable">acceptable</option>
                                                <option value="half_full">bad</option>
                                                <option value="half_empty">half empty</option>
                                                <option value="empty">empty</option>
                                            </select>
                                        </div>
                                        
                                    </div>
                                    <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Air Filter</label>
                                                <select class="form-control-lg" id="air_filter" name="air_filter" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="very_good">very good</option>
                                                    <option value="half_full">bad</option>
                                                    <option value="half_empty">half empty</option>
                                                    <option value="empty">empty</option>
                                                </select> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Voltage Condition</label>
                                                <input name= "battery_voltage_physical_condition" class="form-control" id="battery_voltage_physical_condition"  required> 
                                            </div>
                                        </div>
                                        
                                    </div> 
                                    <div class="row">
										<div class="col">
                                            <label for="exampleFormControlSelect1"> Fan Belt </label>
                                            <select class="form-control-lg" id="fan_belt" name="fan_belt" required>
                                                <option selected disabled>-----</option>
                                                <option value="good">good</option>
                                                <option value="bad">bad</option>
                                                <option value="half_empty">half empty</option>
                                                <option value="empty">empty</option>
                                            </select>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Battery Resistance</label>
                                                <select class="form-control-lg" id="battery_resistance" name="battery_resistance" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="strong">strong</option>
                                                    <option value="half_full">bad</option>
                                                    <option value="half_empty">half empty</option>
                                                    <option value="empty">empty</option>
                                                </select> 
                                            </div>
                                        </div>
                                        
                                    </div>
                                    <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Cleaning Unit Exterior</label>
                                                <select class="form-control-lg" id="cleaning_unit_exterior" name="cleaning_unit_exterior" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="True">True</option>
                                                    <option value="False">False</option>
                                                    
                                                </select> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Fuel Cleaning</label>
                                                <select class="form-control-lg" id="fuel_cleaning" name="fuel_cleaning" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="True">True</option>
                                                    <option value="False">False</option>
                                                    
                                                </select> 
                                            </div>
                                        </div>
                                        
                                    </div>
                                    <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Oil Change</label>
                                                <select class="form-control-lg" id="oil_change" name="oil_change" required>
                                                    <option selected disabled>-----</option>
                                                    <option value="True">True</option>
                                                    <option value="False">False</option>
                                                    
                                                </select> 
                                            </div>
                                        </div>
                                        
                                        
                                    </div> 
                                        
									  

									  

									 
									  
									  
									  
				
									  
