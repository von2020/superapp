<body>
	<div class="main-wrapper">

       <!-- Insert the sidebar partial here-->
	   <%- include('./bars/admin_sidebar') %>
	
	   <div class="page-wrapper">
			   
	   <!-- partial:partials/_navbar.html, this represents the topbar bar -->
	   <%- include('./bars/admin_topbar') %>
	   <!-- partial -->

		   <div class="page-content">
			   <%- include('.././partials/messages')%>
								
				<div class="row">
					<div class="col-md-12 grid-margin stretch-card">
						<div class="card">
							<div class="card-body">
								<h6 class="card-title">Add Generator</h6>

								
                                <form class="" action="/admin/manage/addGenerator" method="post">
                                    
                                    <div class="row">
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Serial Number</label>
                                                <input name= "serial_number" class="form-control" id="serial_number"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"> Generator Maker</label>
                                                <input name= "gen_maker" class="form-control" id="gen_maker"  required>
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Generator Model</label>
                                                <input name= "gen_model" class="form-control" id="gen_model"  required>
                                            </div>
                                        </div>
									  </div>
									  
									  <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Location</label>
                                                <input name= "location" class="form-control" id="location"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Electrical Rating(kilowats) </label>
                                                <input name= "kilowatt_electrical_rating" class="form-control" id="kilowatt_electrical_rating"  required> 
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Electrical Rating(kilovoltsAMPS) </label>
                                                <input name= "kilovoltamp_electrical_rating" class="form-control" id="kilovoltamp_electrical_rating"  required> 
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
										<div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Installation Date</label>
                                                <input name="installation_date" type="date" value="2020-01-01" class="form-control" id="installation_date" required> 
                                            </div>
                                        </div>
                                        
                                        
                                        <div class="col">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1">Next Servicing Date</label>
                                                <input name="next_servicing" type="date" value="2020-01-01" class="form-control" id="next_servicing" required> 
                                            </div>
                                        </div>
                                    </div>
                                        
									  

									  

									 
									  
									  
									  
				
									  
								<!-- need to come back here to add the list of all the subsidiaries and uplines in case it needs to be changed, just copy from the old registration page-->
									<button class="btn-lg btn-primary" type="submit">Save</button>
									
								</form>
								
							</div>
						</div>
					</div>
				</div>

			</div>
			
			<!-- partial:partials/_footer.html -->
            <%- include('./bars/admin_footer') %>
			<!-- partial -->
	
		</div>
	</div>

